{
  "name": "hview-ui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "husky install",
    "dev": "pnpm run --filter ./examples dev",
    "build": "gulp -f ./script/pkg-build/gulpfile.ts & gulp -f ./script/remove/gulpfile.ts",
    "publish": "cd ./packages/components/hview-ui & pnpm run release",
    "publish:build": "gulp -f packages/components/script/publish/gulpfile.ts",
    "publish:gulp": "pnpm run build & gulp -f script/publish/gulpfile.ts",
    "docs:dev": "pnpm run --filter ./site docs:dev",
    "docs:build-other": "pnpm run --filter ./site docs:build",
    "docs:build": "gulp -f script/vite-build/gulpfile.ts",
    "play:build": "pnpm run --filter ./packages/playground build",
    "lint:eslint": "eslint .",
    "lint:ls-lint": "ls-lint",
    "lint:css": "stylelint packages/theme-chalk/src/**/*.less --fix",
    "lint:prettier": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "lint-staged": "lint-staged",
    "test": "pnpm run --filter ./packages test",
    "coverage": "pnpm run --filter ./packages coverage"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hview-plus/components": "workspace: *",
    "@hview-plus/directives": "workspace: *",
    "@hview-plus/constants": "workspace: *",
    "@hview-plus/hooks": "workspace: *",
    "@hview-plus/locale": "workspace: *",
    "@hview-plus/theme-chalk": "workspace: *",
    "@hview-plus/utils": "workspace: *",
    "@hview-plus/test-utils": "workspace: *",
    "vue-i18n": "9.3.0-beta.16"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@ls-lint/ls-lint": "^1.11.2",
    "@release-it/conventional-changelog": "^5.1.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^16.2.14",
    "@types/gulp": "^4.0.10",
    "@types/gulp-autoprefixer": "^0.0.33",
    "@types/gulp-less": "^0.0.32",
    "jsdom": "16.4.0",
    "@types/less": "^3.0.3",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
