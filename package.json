    "sucrase": "^3.29.0",
    "typescript": "^4.9.4",
    "unplugin-vue-define-options": "^0.12.2",
    "vite": "3.0.9",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-libcss": "^1.0.5",
    "vitest": "0.12.6",
    "vue": "^3.2.45"
